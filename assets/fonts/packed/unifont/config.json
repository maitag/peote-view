{
	"packed": true, // glyphes packed into textureatlas with ttfcompile of gl3font (see here: https://github.com/deltaluca/gl3font) 
	"distancefield": true, // distancefield-rendering ( use ttfcompile to generate distancefields )
	
	"kerning": false,
	"width" : 16,
	"height": 16,
	"color" : 0x50e040ff,
	
	"rangeSplitSize": 0x1000, // multiplikator for faster glyphfinding, has to match with the range-startpoints
	
	// multiple unicode ranges (needs @multiSlot and/or @multiTexture for GlyphStype class)
	"ranges":[
		{"image":"unifont_0000-0fff.png", "slot":{"width":2048, "height":2048}, "range":{"min":0x0000, "max":0x0fff}},
		{"image":"unifont_1000-1fff.png", "slot":{"width":2048, "height":2048}, "range":{"min":0x1000, "max":0x1fff}}
	]

}